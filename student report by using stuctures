#include <stdio.h> 
 struct student  
 { 
     char name[50]; 
     int roll; 
     float marks; 
 } ; 
 int main()  
 { 
     int i,n; 
     printf("Enter the value of n = "); 
     scanf("%d",&n); 
     struct student s[n]; 
     if(n>0) 
     { 
             for (i = 0; i < n; i++)  
                 { 
                 printf("\nroll number= \n"); 
                 scanf("%d",&s[i].roll); 
                 printf("Enter first name: "); 
                 scanf("%s", s[i].name); 
                 printf("Enter marks: "); 
                 scanf("%f", &s[i].marks); 
             } 
             if(s[i].name>0 && s[i].roll<0) 
             { 
                     printf("Invalid input"); 
                 } 
             else 
                     { 
                     for (i = 0; i < n; i++)  
                         { 
                         printf("\nRoll number: %d", s[i].roll); 
                         printf("\nFirst name: %s",s[i].name); 
                         printf("\nMarks: %.f", s[i].marks); 
                         printf("\n"); 
                     } 
             }  
         } 
         else 
         { 
                 printf("Invalid input"); 
         } 
     return 0; 
 }
