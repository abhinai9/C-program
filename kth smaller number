#include <stdio.h>
#include <stdlib.h>
int cmpfunc(const void* a, const void* b)
{
	return (*(int*)a - *(int*)b);
}
int kthSmallest(int arr[], int N, int K)
{
	qsort(arr, N, sizeof(int), cmpfunc);
	return arr[K - 1];
}
int main()
{
	int arr[] = { 44, 34, 21, 89, 12 };
	int N = sizeof(arr) / sizeof(arr[0]), K = 3;
	printf("K'th smallest element is %d",
		kthSmallest(arr, N, K));
	return 0;
}
